        <CustomAction Id="CondorConfigLocal" Script="vbscript">
          
            Function CreateConfig()
              Set fso = CreateObject("Scripting.FileSystemObject")
              path = Session.Property("INSTALLDIR")
              Set installpath = fso.GetFolder(path)
              strippedPath = installpath.ShortPath
              Set scriptFile = fso.OpenTextFile(strippedPath &amp; "\etc\WiX\config.vbs",1)
              sText = scriptFile.ReadAll
              scriptFile.close
              
              ExecuteGlobal sText
            End Function
            
            CreateConfig
           
        </CustomAction>
